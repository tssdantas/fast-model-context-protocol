{
  "theme": "GitHub",
  "mcpServers": {
    "mainAPIServer": {
      "command": "python",
      "args": ["main.py", "stdio"]
    }
  },
  "telemetry": {
    "enabled": false
  },
  "usageStatisticsEnabled": true,
  "hideTips": false,
  "registries": {
    "docker.io": {
      "username": "${DOCKER_USERNAME}",
      "pat": "${DOCKER_PAT}"
    }
  }
}